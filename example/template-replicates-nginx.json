{
  "appName": "foobar.nginx",
  "appVersion": "version1",
  "cmd": null,
  "args": null,
  "cpus": 0.2,
  "mem": 300,
  "disk": 100,
  "runAs": "xcm",
  "priority": 100,
  "instances": 5,
  "constraints":"" ,
  "container": {
    "docker": {
      "image": "rails-app",
      "network": "bridge",
      "forcePullImage": false,
      "privileged": true,
      "parameters": [
        {
            "key": "label",
	    "value": "APP_ID=nginx"
        }
      ],
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp",
          "name": "web"
        }
      ]
    },
    "type": "DOCKER",
    "volumes": [
      {
        "hostPath": "/home",
        "containerPath": "/data",
        "mode": "RW"
      }
    ]
  },
  "env": {
  },
  "uris": [
  ],
  "label": {
    "USER_ID": "1"
  },
  "healthCheck":
    {
      "protocol": "http",
      "path": "/",
      "delaySeconds": 10,
      "gracePeriodSeconds": 1,
      "intervalSeconds": 1,
      "portName": "web",
      "timeoutSeconds": 2,
      "consecutiveFailures": 10
    },
  "cmd": "rails server -b 0.0.0.0"
}

